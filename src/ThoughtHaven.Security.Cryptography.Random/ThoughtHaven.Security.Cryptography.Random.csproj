    <RootNamespace>ThoughtHaven.Security.Cryptography</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Thought Haven,Brendan Ridenour</Authors>
    <Company>Thought Haven</Company>
    <Product>ThoughtHaven Security Cryptography Random</Product>
    <Copyright>© Thought Haven. All rights reserved.</Copyright>
    <Description>A simple wrapper class for producing cryptographic random numbers.</Description>
    <Version>0.200.0</Version>
